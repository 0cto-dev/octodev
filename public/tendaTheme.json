{
  "rules": [
    {
      "token": "",
      "foreground": "#f7f8ff",
      "background": "#05050d"
    },
    {
      "token": "keyword",
      "foreground": "#8868ff"
    },
    {
      "token": "variable.language",
      "foreground": "#8868ff"
    },
    {
      "token": "comment",
      "foreground": "#8d8e9b",
      "fontStyle": "italic"
    },
    {
      "token": "string",
      "foreground": "#7a97fb"
    },
    {
      "token": "number",
      "foreground": "#00c4a3"
    },
    {
      "token": "constant",
      "foreground": "#a632bf"
    },
    {
      "token": "function",
      "foreground": "#7a97fb"
    },
    {
      "token": "type",
      "foreground": "#8868ff"
    },
    {
      "token": "class",
      "foreground": "#7a97fb"
    },
    {
      "token": "identifier",
      "foreground": "#f7f8ff"
    },
    {
      "token": "type.identifier",
      "foreground": "#7a97fb"
    },
    {
      "token": "keyword.control",
      "foreground": "#8868ff"
    },
    {
      "token": "punctuation",
      "foreground": "#ff0000"
    },
    {
      "token": "keyword.other",
      "foreground": "#7a97fb"
    },
    {
      "token": "meta.preprocessor",
      "foreground": "#8868ff"
    },
    {
      "token": "invalid",
      "foreground": "#e7000b"
    },
    {
      "token": "disabled",
      "foreground": "#8d8e9b"
    },
    {
      "token": "variable.other",
      "foreground": "#ed68ae" 
    },
    {
      "token": "variable.language",
      "foreground": "#ed68ae"
    },
    {
      "token": "tag",
      "foreground": "#8868ff"
    },
    {
      "token": "link",
      "foreground": "#7a97fb",
      "fontStyle": "underline"
    }
  ],
  "colors": {
    "editor.foreground": "#f7f8ff",
    "editor.background": "#201f34",
    "editor.selectionBackground": "#2a264b",
    "editor.inactiveSelectionBackground": "#444d56",
    "editorCursor.foreground": "#f7f8ff",
    "editorWhitespace.foreground": "#f7f8ff",
    "editorIndentGuide.background": "#f7f8ff",
    "editorIndentGuide.activeBackground": "#f6f8fa"
  }
}
